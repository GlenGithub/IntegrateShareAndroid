<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffeeeeee" android:endColor="#ffeeeeee" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffeeeeee" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ff279ce7" android:endColor="#ff279ce7" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff279ce7" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff279ce7" android:endColor="#ff279ce7" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>
